"*":
  "autocomplete-plus":
    enableAutoActivation: false
    maxVisibleSuggestions: 8
  core:
    closeDeletedFileTabs: true
    customFileTypes:
      "source.cmake": [
        "CMakeLists.txt"
      ]
      "source.octave": [
        "m"
      ]
      "source.shell": [
        "aliasrc"
        "rosrc"
      ]
      "source.yaml": [
        "rviz"
      ]
      "text.xml": [
        "launch"
      ]
    disabledPackages: [
      "spell-check"
      "wrap-guide"
      "teletype"
      "output-panel"
      "dbg-gdb"
      "dbg"
    ]
    packagesWithKeymapsDisabled: [
      "move-panes"
    ]
    projectHome: "/home/seth/BYU-Mars-Rover/"
    telemetryConsent: "limited"
    themes: [
      "gruvbox-dark-ui"
      "gruvbox-mellow"
    ]
  "disable-keybindings":
    prefixKeys: [
      "ctrl-k"
    ]
  editor:
    fontSize: 18
    showIndentGuide: true
    tabLength: 4
    tabType: "soft"
  "exception-reporting":
    userId: "27ba0f0a-acd2-4c24-8aa1-8d3d29fe1e8e"
  "one-dark-ui": {}
  "one-light-ui":
    tabSizing: "Minimum"
  script: {}
  "tree-view": {}
  welcome:
    showOnStartup: false
".text.xml":
  editor:
    tabLength: 2
".text.xml.xsl":
  editor:
    tabLength: 42

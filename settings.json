{
    "files.autoSave": "afterDelay",
    "terminal.integrated.fontFamily": "'Ubuntu Mono', 'PowerlineSymbols'",
    "terminal.integrated.fontSize": 15,
    // "terminal.integrated.shellArgs.linux": ["-c", "tmux"],
    "files.exclude": {
        "**/*__pycache__": true,
        "**/*.clang_complete": true,
        "**/*.github/": true,
        "**/*.gitignore": true,
        "**/*.gitmodules": true,
        "**/*.pdf": true,
        "**/*.pyc": true,
        "**/*.swp": true,
        "**/*.vscode": true,
        "**/*.zip": true,
        "**/*CMakeLists.txt.user": true,
        "**/*rosflight_memory": true,
        "devel/": true,
        "**/*.o": true,
        "**/*.catkin_workspace": true,
        "**/*.~*": true,
    },
    "files.associations": {
        "*.*rc": "shellscript",
        "*.launch": "xml",
        "CMakeLists.txt": "cmake"
    },
    "explorer.autoReveal": false,
    "workbench.tree.indent": 20,
    "explorer.openEditors.visible": 0,
    "editor.minimap.enabled": false,
    "atomKeymap.promptV3Features": true,
    "editor.multiCursorModifier": "alt",
    "workbench.editor.tabSizing": "shrink",
    "window.zoomLevel": 1,
    "keyboard.dispatch": "keyCode",
    "python.pythonPath": "/home/seth/.virtualenvs/default/bin/python3",
    "python.venvPath": "~/.virtualenvs",
    "python.terminal.activateEnvironment": false,
    "workbench.activityBar.visible": false,
    "editor.scrollBeyondLastLine": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorTheme": "Gruvbox Dark Hard - Custom",
    "workbench.iconTheme": "file-icons",
    "zenMode.hideLineNumbers": false,
    "zenMode.hideTabs": false,
    "editor.cursorSmoothCaretAnimation": true,
    "editor.acceptSuggestionOnEnter": "smart",
    "activitusbar.activeColour": "#4989b6",
    "activitusbar.toggleSidebar": false,
    "clock.dateFormat": "h:MM TT",
    "editor.occurrencesHighlight": false,
    "breadcrumbs.enabled": false,
    "python.linting.lintOnSave": true,
    "python.linting.pylintEnabled": true,
    "python.autoComplete.extraPaths": ["/home/seth/dev/boat_landing_sim/src/rosflight_holodeck/python"],
    "C_Cpp.default.intelliSenseMode": "msvc-x64",
    "editor.rulers": [90],
    "macros": {
        "down10Lines": [
            "cursorDown",
            "cursorDown",
            "cursorDown",
            "cursorDown",
            "cursorDown",
            "cursorDown",
            "cursorDown",
            "cursorDown",
            "cursorDown",
            "cursorDown",
        ],
        "up10Lines": [
            "cursorUp",
            "cursorUp",
            "cursorUp",
            "cursorUp",
            "cursorUp",
            "cursorUp",
            "cursorUp",
            "cursorUp",
            "cursorUp",
            "cursorUp"
        ],
        "closePanelSidebar": [
            "workbench.action.toggleSidebarVisibility",
            "workbench.action.closePanel"
        ],
        "buildTaskImproved": [
            "workbench.action.tasks.build",
                {"command": "$delay", "args": {"milliseconds": 500}},
            "workbench.action.terminal.focus"
        ]
    },
    "scm.diffDecorationsGutterWidth": 1,
    "explorer.confirmDragAndDrop": false,
    "editor.fontSize": 12.5,
    "python.jediEnabled": false,
    "editor.suggestSelection": "first",
    "C_Cpp.default.cppStandard": "c++17",
    "explorer.confirmDelete": false,
    "vsicons.dontShowNewVersionMessage": true,
    "workbench.statusBar.visible": true,
    "editor.renderWhitespace": "none",
    "bracket-pair-colorizer-2.showHorizontalScopeLine": false,
    "bracket-pair-colorizer-2.colors": [
        "Gold",
        "Olive",
        "Teal",
    ],
    "window.menuBarVisibility": "hidden",
    "liveshare.featureSet": "insiders",
    "editor.formatOnPaste": true
}

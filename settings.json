{
    "files.autoSave": "afterDelay",
    "terminal.integrated.fontFamily": "'Ubuntu Mono', 'PowerlineSymbols'",
    "terminal.integrated.fontSize": 16,
    "terminal.external.linuxExec": "gnome-terminal",
    "files.exclude": {
        "**/*__pycache__": true,
        "**/*.clang_complete": true,
        "**/*.github/": true,
        "**/*.gitignore": true,
        "**/*.gitmodules": true,
        "**/*.pdf": true,
        "**/*.pyc": true,
        "**/*.swp": true,
        "**/*.vscode": true,
        "**/*.zip": true,
        "**/*CMakeLists.txt.user": true,
        "**/*rosflight_memory": true
    },
    "files.associations": {
        "*.*rc": "shellscript",
        "*.launch": "xml",
        "CMakeLists.txt": "cmake"
    },
    "explorer.autoReveal": false,
    "workbench.tree.indent": 20,
    "explorer.openEditors.visible": 0,
    "editor.minimap.enabled": false,
    "atomKeymap.promptV3Features": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "workbench.editor.tabSizing": "shrink",
    "window.zoomLevel": 1,
    "keyboard.dispatch": "keyCode",
    "python.pythonPath": "/home/seth/.virtualenvs/default/bin/python3",
    "python.terminal.executeInFileDir": true,
    "python.venvPath": "~/.virtualenvs",
    "python.terminal.activateEnvironment": false,
    "editor.fontSize": 13,
    "workbench.activityBar.visible": false,
    "window.menuBarVisibility": "default",
    "editor.scrollBeyondLastLine": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorTheme": "Gruvbox Dark - Custom",
    "workbench.iconTheme": "file-icons",
    "zenMode.hideLineNumbers": false,
    "zenMode.hideTabs": false,
    "editor.cursorSmoothCaretAnimation": true,
    "editor.acceptSuggestionOnEnter": "smart",
    "activitusbar.activeColour": "#4989b6",
    "activitusbar.toggleSidebar": false,
    "clock.dateFormat": "h:MM TT",
    "editor.folding": false,
    "editor.occurrencesHighlight": false,
    "breadcrumbs.enabled": false,
    "python.jediEnabled": false,
    "python.linting.lintOnSave": true,
    "python.linting.pylintEnabled": true,
    "C_Cpp.default.intelliSenseMode": "msvc-x64",
    "macros": {
        "down10Lines": [
            "cursorDown",
            "cursorDown",
            "cursorDown",
            "cursorDown",
            "cursorDown",
            "cursorDown",
            "cursorDown",
            "cursorDown",
            "cursorDown",
            "cursorDown",
        ],
        "up10Lines": [
            "cursorUp",
            "cursorUp",
            "cursorUp",
            "cursorUp",
            "cursorUp",
            "cursorUp",
            "cursorUp",
            "cursorUp",
            "cursorUp",
            "cursorUp",
        ],
        "closePanelSidebar": [
            "workbench.action.toggleSidebarVisibility",
            "workbench.action.closePanel"
        ]
    },
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
}
{
    "files.autoSave": "afterDelay",
    "terminal.integrated.fontFamily": "'Ubuntu Mono', 'PowerlineSymbols'",
    "terminal.integrated.fontSize": 16,
    "terminal.external.linuxExec": "gnome-terminal",
    "files.exclude": {
        "**/*.zip": true,
        "**/*.pyc": true,
        "**/*__pycache__": true,
        "**/*.swp": true,
        "**/*.vscode": true,
        "**/*.pdf": true,
    },
    "files.associations": {
        ".aliasrc": "shellscript",
        ".rosrc": "shellscript",
        "*.launch": "xml",
        "CMakeLists.txt": "cmake"
    },
    "explorer.autoReveal": false,
    "workbench.tree.indent": 20,
    "explorer.openEditors.visible": 0,
    "editor.minimap.enabled": false,
    "atomKeymap.promptV3Features": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.formatOnPaste": true,
    "workbench.editor.tabSizing": "shrink",
    "window.zoomLevel": 1,
    "keyboard.dispatch": "keyCode",
    "python.pythonPath": "/home/seth/.virtualenvs/default/bin/python3",
    "python.terminal.executeInFileDir": true,
    "python.venvPath": "~/.virtualenvs",
    "python.terminal.activateEnvironment": false,
    "editor.fontSize": 13,
    "workbench.activityBar.visible": false,
    "window.menuBarVisibility": "hidden",
    "editor.scrollBeyondLastLine": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorTheme": "Gruvbox Dark - Custom",
    "workbench.iconTheme": "file-icons",
    "zenMode.hideLineNumbers": false,
    "zenMode.hideTabs": false,
    "editor.cursorSmoothCaretAnimation": true,
    "editor.acceptSuggestionOnEnter": "smart",
    "activitusbar.activeColour": "#4989b6",
    "activitusbar.toggleSidebar": false,
    "clock.dateFormat": "h:MM TT",
    "editor.folding": false,
    "editor.occurrencesHighlight": false
}
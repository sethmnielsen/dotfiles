Xcursor.size:16

! custom urxvt theme
! black
URxvt.color0  : #01060E
URxvt.color8  : #686868
! red
URxvt.color1  : #EA6C73
URxvt.color9  : #F07178
! green
URxvt.color2  : #91B362
URxvt.color10 : #C2D94C
! yellow
URxvt.color3  : #F9AF4F
URxvt.color11 : #FFB454
! blue
URxvt.color4  : #53BDFA
URxvt.color12 : #59C2FF
! magenta
URxvt.color5  : #FAE994
URxvt.color13 : #FFEE99
! cyan
URxvt.color6  : #90E1C6
URxvt.color14 : #95E6CB
! white
URxvt.color7  : #C7C7C7
URxvt.color15 : #FFFFFF

! '#01060E','#EA6C73','#91B362','#F9AF4F','#53BDFA','#FAE994','#90E1C6','#C7C7C7',
! '#686868','#F07178','#C2D94C','#FFB454','#59C2FF','#FFEE99','#95E6CB','#FFFFFF'

! foreground/background
URxvt*background: #17191C
URxvt*depth: 32
URxvt*foreground: #FFFFFF

URxvt.scrollBar: False

URxvt*font: xft:FantasqueSansMono-Regular:size=13:antialias=true
URxvt*boldFont: xft:FantasqueSansMono-Bold:size=13:antialias=true
URxvt*italicFont: xft:FantasqueSansMono-Italic:size=13:antialias=true
URxvt*letterSpace: -1

URxvt.perl-ext-common: default,font-size
URxvt.keysym.C-equal:   font-size:increase
URxvt.keysym.C-minus:   font-size:decrease

URxvt.iso14755: false
URxvt.iso14755_52: false

URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

URxvt.keysym.Control-Meta-c: builtin-string:
URxvt.keysym.Control-Meta-v: builtin-string:


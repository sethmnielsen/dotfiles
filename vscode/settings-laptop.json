{
"workbench.startupEditor": "none",
"files.autoSave": "afterDelay",
"editor.fontFamily": "'Iosevka', 'monospace'",
"editor.fontLigatures": false,
"terminal.integrated.fontFamily": "'FantasqueSansMono Nerd Font Mono', 'DejaVuSansMono Nerd Font Mono'",
"editor.wordWrapColumn": 90,
"editor.rulers": [90],
"search.useReplacePreview": false,
"C_Cpp.default.cppStandard": "c++11",
"C_Cpp.default.compilerPath": "/usr/bin/gcc",
"C_Cpp.default.includePath": [
    "${workspaceFolder}**",
    "/usr/include/eigen3"
],
"files.exclude": {
    ".env": true,
    "**/*__pycache__": true,
    "**/*.~*": true,
    "**/*.catkin_workspace": true,
    "**/*.code-workspace": true,
    "**/*.clang_complete": true,
    "**/*.github/": true,
    "**/*.gitignore": true,
    "**/*.gitmodules": true,
    "**/*.ipynb_checkpoints": true,
    "**/*.m": true,
    "**/*.mypy_cache": true,
    "**/*.o": true,
    "**/*.ods": true,
    "**/*.odt": true,
    "**/*.pyc": true,
    "**/*.rst": true,
    "**/*.so": true,
    "**/*.swp": true,
    "**/*.tar.gz": true,
    "**/*.zip": true,
    "**/*airbourne": true,
    "**/*breezy": true,
    "**/*CMakeLists.txt.user": true,
    "**/*docs": true,
    "**/*rosflight_memory": true,
    "**/Cargo.lock": true,
    "**/build": true,
    "**/devel": true,
    "**/target/": true,
    "**F450.yaml": true,
    "**/*.pytest_cache/": true,
    "**/*.ui": true
},
"files.associations": {
    "*.*rc": "shellscript",
    "*.launch": "xml",
    "CMakeLists.txt": "cmake",
    "<ipython-input*": "python",
    "*.tex": "latex",
    ".vimrc": "vimrc",
    "*.metadata": "json"
},
"explorer.autoReveal": false,
"workbench.tree.indent": 20,
"explorer.openEditors.visible": 0,
"editor.minimap.enabled": false,
"atomKeymap.promptV3Features": true,
"editor.multiCursorModifier": "alt",
"python.venvPath": "/home/seth/.virtualenvs",
"python.terminal.activateEnvironment": false,
"workbench.activityBar.visible": false,
"workbench.iconTheme": "ayu",
"zenMode.hideLineNumbers": false,
"editor.cursorSmoothCaretAnimation": true,
"editor.acceptSuggestionOnEnter": "off",
"activitusbar.activeColour": "#4989b6",
"activitusbar.toggleSidebar": false,
"breadcrumbs.enabled": true,
// "python.autoComplete.extraPaths": [
//     "/usr/lib/python3.8/site-packages/",
// ],
// "python.analysis.extraPaths": [
//     "/usr/lib/python3.8/site-packages/"
// ],
"macros.list": {
    "closePanelSidebar": [
        "workbench.action.toggleSidebarVisibility",
        "workbench.action.closePanel"
    ],
    "resizePaneRight": [
        "workbench.action.terminal.resizePaneRight"
    ],
    "resizePaneLeft": [
        "workbench.action.terminal.resizePaneLeft"
    ],
    "openTerminalToSide": [
        "workbench.action.positionPanelRight",
        {
            "command": "resizePaneRight",
            "args": {
                "times":4
            }
        },
        {
            "command": "resizePaneLeft",
            "args": {
                "times":4
            }
        }
    ],
    "positionPanelRightTerminalFocus": [
        "workbench.action.positionPanelRight",
        "workbench.action.focusActiveEditorGroup",
        "terminal.focus"
    ],
    "positionPanelBottomTerminalFocus": [
        "workbench.action.positionPanelBottom",
        "terminal.focus"
    ],
    "positionPanelRightEditorFocus": [
        "workbench.action.positionPanelRight",
        "workbench.action.focusActiveEditorGroup"
    ],
    "positionPanelBottomEditorFocus": [
        "workbench.action.positionPanelBottom",
        "workbench.action.focusActiveEditorGroup"
    ],
    "runAllAndRestartKernel": [
        "python.datascience.restartkernel",
        "python.datascience.runallcells",
    ],
    "runAboveAndRestartKernel": [
        "python.datascience.restartkernel",
        "python.datascience.runallcellsabove.palette",
    ],
    "runAboveAndDebugCurrentCell": [
        "python.datascience.restartkernel",
        "python.datascience.runallcellsabove.palette",
        "python.datascience.debugcurrentcell.palette"
    ],
    "focusEditorAndVimCtrlW": [
        "workbench.action.focusActiveEditorGroup",
        "extension.vim_ctrl+w"
    ],
    "saveAndRunCode": [
        "workbench.action.files.save",
        "code-runner.run"
    ]
},
"scm.diffDecorationsGutterWidth": 1,
"explorer.confirmDragAndDrop": false,
"editor.suggestSelection": "first",
"explorer.confirmDelete": false,
"files.watcherExclude": {
    "/home/seth/.local/share/**": true,
    "/home/seth/Downloads/**": true
},
"workbench.sideBar.location": "left",
"workbench.list.automaticKeyboardNavigation": false,
"vsicons.dontShowNewVersionMessage": true,
"activitusbar.searchViewInPanel": false,
"git.enableStatusBarSync": false,
"gitlens.views.repositories.location": "explorer",
"gitlens.views.fileHistory.location": "gitlens",
"gitlens.views.lineHistory.location": "gitlens",
"gitlens.views.compare.location": "gitlens",
"gitlens.views.search.location": "gitlens",
"gitlens.codeLens.authors.enabled": false,
"gitlens.codeLens.recentChange.enabled": false,
"gitlens.currentLine.enabled": false,
"gitlens.advanced.messages": {
    "suppressGitDisabledWarning": true
},
"gitlens.statusBar.reduceFlicker": true,
"diffEditor.renderSideBySide": true,
"problems.decorations.enabled": true,
"markdown.preview.breaks": true,
"markdown.preview.markEditorSelection": false,
"todohighlight.keywords": [
    {
        "text": "HACK:",
        "color": "#000",
        "isWholeLine": false,
    },
    {
        "text": "NOTE:",
        "color": "turquoise",
        "backgroundColor": "none",
    },
    {
        "text": "TODO:",
        "color": "crimson",
        "backgroundColor": "none",
        // "border": "1px solid red",
        // "borderRadius": "2px", //NOTE: using borderRadius along with `border` or you will see nothing change
    }
],
"shellformat.effectLanguages": [
    "shellscript",
    "dockerfile",
    "dotenv",
    "hosts",
    "jvmoptions",
    "ignore",
    "gitignore",
    "properties",
    "spring-boot-properties",
    "azcli"
],
"window.customMenuBarAltFocus": false,
"window.enableMenuBarMnemonics": false,
"matlab.lintOnSave": false,
"editor.snippetSuggestions": "bottom",
"rust.show_warnings": false,
"extensions.autoUpdate": false,
"editor.links": false,
"code-runner.executorMap": {
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "python": "ipython3",
    "shellscript": "bash",
    "rust": "cd $dir && cargo run --release"
},
"code-runner.runInTerminal": true,
"cmake.generator": "Unix Makefiles",
"cmake.configureOnOpen": false,
"cmake.skipConfigureIfCachePresent": true,
"cmake.configureOnEdit": false,
"cmake.statusbar.visibility": "compact",
"cmake.statusbar.advanced": {
    "buildTarget": {"visibility": "hidden"},
    "ctest": {"visibility": "hidden"},
    "debug": {"visibility": "hidden"},
    "kit": {"visibility": "hidden"},
    "launch": {"visibility": "hidden"}
},
"[latex]": {
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 100,
    "editor.tabSize": 4,
    "editor.suggest.showSnippets": true,
    "editor.snippetSuggestions": "inline"
},
// "latex-workshop.hover.preview.newcommand.newcommandFile": "/home/seth/texmf/tex/latex/resume/documentMETADATA.cls",
"latex-workshop.chktex.enabled": true,
"latex-workshop.chktex.args.root": [ "-wall", "-n22", "-n30", "-n46", "-e16", "-q" ],
"latex-workshop.chktex.args.active": [ "-wall", "-n9", "-n17", "-n22", "-n30", "-n45", "-n46", "-e16", "-q" ],
"latex-workshop.chktex.run": "onType",
"latex-workshop.chktex.delay": 2000,
"latex-workshop.latex.autoBuild.run": "never",
"latex-workshop.message.update.show": false,
"latex-workshop.view.pdf.viewer": "external",
"latex-workshop.latex.outDir": "%DIR%",
"latex-workshop.latex.recipe.default": "first",
"latex-workshop.view.pdf.hand": true,
"latex-workshop.view.pdf.trim": 1,
"latex-workshop.message.error.show": false,
"latex-workshop.message.warning.show": false,
"latex-workshop.view.pdf.backgroundColor": "#e6e6e6",
"latex-workshop.message.log.show": false,
"latex-workshop.latex.tools": [
    {
        "name": "lualatexmk",
        "command": "latexmk",
        "args": [
            "-synctex=1",
            "-interaction=nonstopmode",
            "-file-line-error",
            "-lualatex",
            "-outdir=%OUTDIR%",
            "%DOC%"
        ],
        "env": {}
    },
    {
        "name": "latexmk",
        "command": "latexmk",
        "args": [
            "-pdf",
            "%DOC%"
        ],
        "env": {}
    },
    {
        "name": "biber",
        "command": "biber",
        "args": [
            "%OUTDIR%/%DOCFILE%"
        ],
        "env": {}
    },
    {
        "name": "bibtex",
        "command": "bibtex",
        "args": [
            "%OUTDIR%/%DOCFILE%"
        ],
        "env": {}
    }
],
"latex-workshop.latex.recipes": [
    {
        "name": "lualatexmk",
        "tools": [
            "lualatexmk"
        ]
    },
    {
        "name": "latexmk",
        "tools": [
            "latexmk"
        ]
    },
    {
        "name": "biber ➞ lualatexmk",
        "tools": [
            "biber",
            "lualatexmk"
        ]
    },
    {
        "name": "pdflatex ➞ bibtex ➞ pdflatex × 2",
        "tools": [
            "pdflatex",
            "bibtex",
            "pdflatex",
            "pdflatex"
        ]
    }
],
"vim.vimrc.enable": true,
"vim.vimrc.path": "/home/seth/env/.vimrc",
"vim.useCtrlKeys": true,
// "vim.sneak": false,
// "vim.sneakReplacesF": true,
// "vim.whichwrap": "h,l,<,>,[,]",
"vim.textwidth": 90,
"vim.smartcase": false,
"vim.ignorecase": false,
"vim.foldfix": true,
"vim.useSystemClipboard": true,
"vim.searchHighlightColor": "gray",
"vim.incsearch": true,
"vim.visualstar": true,
// "vim.debug.silent": false,
// "vim.debug.loggingLevelForAlert": "debug",
"vim.autoindent": true,
"vim.handleKeys": {
    "<C-d>": false,
    "<C-/>": false,
    "<C-z>": false,
    "<C-Z>": false,
    "<C-]>": false,
    "<C-[>": false,
    "u": false,
    "<C-y>": true,
    "Up": true,
    "Down": true,
    "Left": true,
    "Right": true,
    "<C-k>": false,
},
"vim.insertModeKeyBindingsNonRecursive": [
    {
        "before": ["<alt-k>"],
        "after": ["g", "k"]
    },
    {
        "before": ["<alt-j>"],
        "after": ["g", "j"]
    },
    // {
    //     "before": ["<leader>","c","i"],
    //     "commands": ["editor.action.commentLine"]
    // },
],
"vim.normalModeKeyBindingsNonRecursive": [
    {
        "before": ["k"],
        "after": ["g", "k"]
    },
    {
        "before": ["j"],
        "after": ["g", "j"]
    },
    {
        "before": ["o"],
        "commands": ["editor.action.insertLineAfter","extension.vim_insert"]
    },
    {
        "before": ["O"],
        "commands": ["editor.action.insertLineBefore","extension.vim_insert"]
    },
    {
        "before": ["Z","Z"],
        "commands": [":wq"]
    },
    {
        "before": ["Q","Q"],
        "commands": ["workbench.action.closeActiveEditor"]
    },
    {
        "before": ["<leader>","c","i"],
        "commands": ["editor.action.commentLine"]
    },
    {
        "before": ["K"],
        "after": ["1","0","-"]
    },
    {
        "before": ["J"],
        "after": ["1","0","+"]
    },
    {
        "before": ["u"],
        "commands": ["undo"]
    },
    {
        "before": ["<C-r>"],
        "commands": ["redo"]
    },
    // {
    //     "before": ["S"],
    //     "after": ["y", "s", "a", "W", "$"]
    // },
],
"vim.visualModeKeyBindingsNonRecursive": [
    {
        "before": ["k"],
        "after": ["g", "k"]
    },
    {
        "before": ["j"],
        "after": ["g", "j"]
    },
    {
        "before": ["K"],
        "after": ["1","0","-"]
    },
    {
        "before": ["J"],
        "after": ["1","0","+"]
    },
    {
        "before": [ "p"],
        "after": [ "p", "g", "v", "y" ]
    },
    {
        "before": ["u"],
        "commands": ["undo"]
    },
    {
        "before": ["<C-r>"],
        "commands": ["redo"]
    },
],
"editor.scrollBeyondLastLine": false,
"editor.smoothScrolling": true,

"editor.accessibilitySupport": "off",
"python.linting.lintOnSave": false,
"python.linting.pylintArgs": [
    "--ignored-modules=cv2,numpy",
    "--disable=all",
    "--enable=F,E,unreachable,duplicate-key,unnecessary-semicolon,global-variable-not-assigned,binary-op-exception,bad-format-string,anomalous-backslash-in-string,bad-open-mode",
    "--disable=unresolved-import"
],
"python.analysis.disabled": ["undefined-variable"],
"python.testing.autoTestDiscoverOnSaveEnabled": false,
"python.testing.promptToConfigure": false,
"terminal.integrated.shell.linux": "/usr/bin/zsh",
"markdown.preview.scrollPreviewWithEditor": false,
"markdown.preview.scrollEditorWithPreview": false,
"files.trimTrailingWhitespace": true,
"diffEditor.ignoreTrimWhitespace": false,
"files.eol": "\n",
"python.languageServer": "Pylance",
"python.defaultInterpreterPath": "/usr/bin/python",
"code-runner.enableAppInsights": false,
"code-runner.showRunCommandInExplorerContextMenu": false,
"code-runner.showRunCommandInEditorContextMenu": false,
"python.showStartPage": false,
"npm.autoDetect": "off",
"window.menuBarVisibility": "hidden",
"update.mode": "manual",
"extensions.ignoreRecommendations": true,
"terminal.integrated.allowChords": false,
"breadcrumbs.filePath": "off",
"workbench.editor.pinnedTabSizing": "shrink",
"workbench.colorTheme": "ayu-seth",
"window.zoomLevel": 0,
"debug.onTaskErrors": "abort",

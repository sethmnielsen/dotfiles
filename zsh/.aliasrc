# Print the 256 color test pattern
print256colors() {
  for i in {0..255} ; do
    printf "\x1b[48;5;%sm%3d\e[0m " "$i" "$i"
    if (( i == 15 )) || (( i > 15 )) && (( (i-15) % 6 == 0 )); then
        printf "\n";
    fi
  done
}

# FFMPEG converting MP4 or MOV to MOV for Resolve video editor
ffmpeg_mp4_to_mov_resolve() {
  if [[ "$#" -ne 1 ]]; then
    echo "Usage: $0 [dir]"
  elif ! [[ -e $1 ]]; then
    echo "$1 does not exist"
  else
    if [[ -d $1 ]]; then
      if [[ "${1: -1}" == "/" ]]; then
        FILES=${1}*
      else
        FILES=${1}/*
      fi
      CONV_DIR=${FILES:0:-2}/converted_resolve
      if [[ -e $CONV_DIR ]]; then
        echo "Error: directory for converted files already exists!"
        return 1
      fi
      mkdir $CONV_DIR
      for f in $~FILES; do
        echo "\n\n ------- Converting file $f -------\n\n"
        if [[ -f $f ]]; then
          fname=${f:2:-4}     \
          fps=$(ffmpeg -i ${f} 2>&1 | sed -n "s/.*, \(.*\) fp.*/\1/p")
                              \
          ffmpeg -i ${f}      \
            -vcodec dnxhd     \
            -acodec pcm_s16le \
            -s 1920x1080      \
            -r $fps           \
            -b:v 45M          \
            -pix_fmt yuv422p  \
            -f mov            \
            "${CONV_DIR}/${fname}_R.MOV"
        fi
      done
    fi
  fi
}
ffmpeg_mov_to_mp4_resolve() {
  if [[ "$#" -ne 1 ]]; then
    echo "Usage: $0 [infile.mov]"
  elif ! [[ -e $1 ]]; then
    echo "$1 does not exist"
  else
    fname=${${1:0:-}}   \

    ffmpeg -i ${1}      \
    -vf yadif           \
    -codec:v libx264    \
    -crf 1              \
    -bf 2               \
    -flags +cgop        \
    -pix_fmt yuv420p    \
    -codec:a aac        \
    -strict -2          \
    -b:a 128k           \
    -r:a 48000          \
    -movflags faststart \
    ${fname}.MP4
  fi
}


  cd $A
  echo "\npersonal repos have been updated on this machine.\n"
}
push_changes() {
  A=$(pwd)
  echo "\n--$name1--"
  cd $dir1
  git add -A
  git commit -m "Auto-pushed changes"
  git push
  echo "\n--$name2--"
  cd $dir2
  git add -A
  git commit -m "Auto-pushed changes"
  git push
  echo "\n--$name3--"
  cd $dir3
  git add -A
  git commit -m "Auto-pushed changes"
  git push
  echo "\n--$name4--"
  cd $dir4

mount_ubuntu() {
    sudo mkdir -p /mnt/ubuntu
    sudo mount /dev/nvme0n1p6 /mnt/ubuntu
    echo "Mounted ubuntu partition (nvme0n1p6) to /mnt/ubuntu"
    echo ""
}
umount_ubuntu() {
    sudo umount /mnt/ubuntu
    sudo rm -rf /mnt/ubuntu
    echo "Unmounted ubuntu parition"
    echo "Deleted '/mnt/ubuntu' mount point"
    echo ""
}

